@inherits UmbracoViewPage<LeasingEnquiry>
@using ContentModels = Umbraco.Web.PublishedModels;
@using System.Threading;
@{
	Layout = "master.cshtml";
}

<link rel="stylesheet" href=@Url.Content("~/css/main.min.css?ver=102") />
@* the fun starts here *@
<style type="text/css">
    div.inner-section {
        padding-bottom: 0px
    }
    div.inner-section {
        margin-top: 100px
    }

    div.leasing-form .btn-black {
        padding: 17px 40px !important;
        background-color: #000 !important;
    }

    div.leasing-form .form-group label {
        color: #45454a !important;
    }
    .title-holder .title{
        font-weight:700;
    }
</style>

@{
    var leasingBanner = Model.Value<IPublishedContent>("leasingBanner");
    if(leasingBanner!=null){
        <div class="container-fluid abt_banner slider-main ">
            <div class="row">
                <div class="main-banner">
                    <div class="banner_info inner_banner_info" style="display: block;width: 100%;">
                        <div class="item a inner_banner_item">
                            <img src="@leasingBanner.Url" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
}
<div class="inner-section">
    <div class="container-fluid custom-container-fluid">
        <div class="row">
            <div class="col-md-12">
                <!--Start Main Title-->
                @Html.Raw(Model.Value("leasingEnquiryMainHeading"))
                <!--End Main Title-->
            </div>
        </div>
    </div>
</div>

<div class="page-content">
    <div class="leasing-form">
        <div class="form-background">
            <div class="container">
                <div class="row">
                    <div class="col-md-10 col-md-offset-1">
                        <div class="background-layer"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="form">
                        @Umbraco.RenderMacro("renderUmbracoForm", new { FormGuid = "41c8a072-5250-4037-a283-85746341543a", FormTheme = "LeasingForm-formtheme", ExcludeScripts = "1" })
                        <div class="field-validation-error" id="divErrorXSS" style="display:none; color:red;">Please a provide valid value</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

