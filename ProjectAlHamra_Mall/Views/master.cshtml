    <style>
        /* CSS FOR DEMO ONLY */

        .movie_carasual .owl-dots {
            width: 10px;
            text-align: center;
            height: auto;
            display: inline-block;
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            right: 30px;
        }

        .movie_carasual .owl-dot {
            width: 8px;
        }

            .movie_carasual .owl-dot span {
                display: inline-block;
                -webkit-border-radius: 0;
                -moz-border-radius: 0;
                border-radius: 0;
                width: 8px;
                height: 5px;
                margin-left: 2px;
                margin-right: 2px;
                background: url(~/Images/nav_dot.png) no-repeat;
                background-position: center;
                background-size: contain;
                border: none;
            }

            .movie_carasual .owl-dot.active span,
            .movie_carasual .owl-controls.clickable .owl-dot:hover span {
                background: url(~/Images/nav_dot.png);
                width: 8px;
                height: 8px;
                background-position: center;
                background-size: contain;
            }
    </style>

    <style>
        .events_cont a.img_contr {
            width: 100%;
        }
    </style>

    <style>
        @@media (max-width:560px) {
            #timeline[data-columns]::before {
                content: '1.column.size-1of1';
            }
        }


        @@media screen and (min-width: 560px) and (max-width: 767px) {
            #timeline[data-columns]::before {
                content: '2.column.size-1of2';
            }
        }



        @@media (min-width: 768px) {
            #timeline[data-columns]::before {
                content: '3 .column.size-1of3';
            }
        }
    </style>
</head>

