@inherits Umbraco.Web.Mvc.UmbracoViewPage
@{
	Layout = "master.cshtml";
}

@* the fun starts here *@

@{
    var blogMainBanner = Model.Value<IPublishedContent>("blogMainBanner");
    if(blogMainBanner!=null){
        <div class="container-fluid abt_banner slider-main " style="background: url(@blogMainBanner.Url())no-repeat bottom center; background-size:110%">
            <div class="row">
                <div class="slider-main-text text-center">
                    <!-- <h1 class="white-text ">LET<br>
                    YOURSELF<br>GO!</h1> -->
                </div>
            </div>
        </div>
    }
}



